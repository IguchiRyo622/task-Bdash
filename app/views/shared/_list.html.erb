<%# shared/team.cssに依存 %>
<div class="list-top">
  <div class="team-new">
    <%= link_to '新規リスト作成', new_team_list_path, class:"teams-create" %>
  </div>
  <div class="teams">
    <%= link_to @team.team_name, team_path(@team.id,@list), class:"team-name" %>
    <% @lists.each do |list| %>
    <div class="team">
      <div>
        <%= link_to list.list_name, team_list_tasks_path(@team.id,list), class:"team-name" %>
      </div>
    </div>
    <% end %>
  </div>
</div>